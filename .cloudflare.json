{
  "worker": {
    "build": {
      "command": "bun run build && npx wrangler deploy",
      "output_dir": "dist",
      "root_dir": ".",
      "env": {
        "NODE_VERSION": "18"
      }
    },
    "deployment": {
      "compatibility_date": "2024-10-02",
      "compatibility_flags": ["nodejs_compat"],
      "deploy_command": "npx wrangler deploy --env production"
    }
  }
}